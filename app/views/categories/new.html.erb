<% provide :title, t(".title") %>
<h1><%= t ".h1" %></h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @category do |f| %>
      <%= render "shared/error_messages", object: f.object %>

      <%= f.label t(".name") %>
      <%= f.text_field :name, class: "form-control" %>

      <%= f.submit t(".add"), class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
